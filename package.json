    "font-awesome": "^4.6.3",
    "headroom.js": "^0.9.3",
    "javascript-natural-sort": "0.7.1",
    "laravel-mix": "^1.0",
    "moment": "^2.14.1",
    "moment-duration-format": "^2.2.2",
    "prop-types": "^15.5.8",
    "vue": "^1.0.28",
